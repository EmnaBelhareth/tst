{
  "id": "com.dynamoTable",
  "name": "Dynamic Table 1222",
  "version": "v1.0",
  "description": "A custom widget to help you create custom tables.",
  "newInstancePrefix": "dynamoTable",
  "vendor": "Emna Belhareth",
  "eula": "",
  "license": "MIT",
  "icon": "https://cdn.jsdelivr.net/gh/SAP-Custom-Widget/dynamoTable@v1.0/icon.png",
  "imports": [
    "datasource",
    "planning-model"
  ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "dynamo-table",
      "url": "https://cdn.jsdelivr.net/gh/EmnaBelhareth/tst@v1.0/dynamoTable.bundle.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "builder",
      "tag": "dynamo-table-builder",
      "url": "https://cdn.jsdelivr.net/gh/EmnaBelhareth/tst@v1.0/dynamoTable.bundle.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "tableTitle": {
      "description": "Name of Table",
      "type": "string",
      "default": "Dynamo Table"
    },
    "searching": {
      "type": "boolean",
      "default": true
    }
  },
  "methods": {
    "setTableData": {
      "description": "Sets the table data.",
      "parameters": [
        {
          "name": "RecordsArray",
          "type": "Selection[]"
        }
      ]
    }
  },
  "events": {}
}
